{
  "name": "sudoku-game",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm:tailwind-watch\"",
    "tailwind-watch": "npx tailwindcss -i ./src/assets/main.css -o ./dist/output.css --watch",
    "build": "npx tailwindcss -i ./src/assets/main.css -o ./dist/output.css && vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@vitejs/plugin-vue": "^5.2.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cssnano": "^7.0.6",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.15",
    "typescript": "~5.6.2",
    "vite": "^6.0.1",
    "vue-tsc": "^2.1.10"
  }
}
